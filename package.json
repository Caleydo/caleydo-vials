{
  "name": "vials",
  "description": "Vials visualizes alternative splicing based on mRNAseq data using Caleydo Web.",
  "version": "0.1.5",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/vials",
  "homepage": "http://vials.io",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_d3":"*",
    "caleydo_core": "*"
  },
  "caleydo": {
    "type": "app",
    "plugins": {
      "web": [
        {
          "type": "datatype",
          "id": "caleydo-genome-data-link",
          "file": "caleydo-genome-data-link"
        },
        {
          "type": "vis",
          "id": "vials-isoforms",
          "name": "Alternative Splicing - IsoForms",
          "size": [
            840,
            300
          ],
          "filter": "caleydo-genome-data-link",
          "file": "vials-isoforms"
        },
        {
          "type": "vis",
          "id": "vials-junctions",
          "name": "Alternative Splicing - Junctions",
          "size": [
            840,
            2000
          ],
          "filter": "caleydo-genome-data-link",
          "file": "vials-junctions-v2"
        },
        {
          "type": "vis",
          "id": "vials-reads",
          "name": "Alternative Splicing - Read Abundance",
          "size": [
            840,
            2000
          ],
          "filter": "caleydo-genome-data-link",
          "file": "vials-read-v2"
        },
           {
          "type": "vis",
          "id": "vials-axis",
          "name": "Alternative Splicing - Axis",
          "size": [
            840,
            2000
          ],
          "filter": "caleydo-genome-data-link",
          "file": "vials-genome-axis"
        },
        {
          "type": "app",
          "name": "Vials"
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "js-lru": "http://github.com/rsms/js-lru.git",
        "bootstrap": "~3",
        "underscore":"~1.8",
        "lodash":">=3.10",
        "font-awesome":">=4.3.0",
        "jquery":">=1.11.0",
        "selectivity":">=2.0"
      }
    },
    "requirejs-config": {
      "paths": {
        "js-lru": "${baseUrl}/js-lru/lru"
      },
      "shim": {
        "js-lru": {
          "exports": "LRUCache"

        }
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
